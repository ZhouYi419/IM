<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zy.im.infrastructure.mapper.FriendRelationMapper">
    <insert id="insert">
        INSERT INTO friend_relation (uuid, friend_uuid,status)
        VALUES (#{uuid}, #{friendUuid},#{status})
    </insert>
</mapper>
